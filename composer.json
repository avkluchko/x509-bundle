{
    "name": "avkluchko/x509-bundle",
    "description": "Bundle for work with x509 certificate",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Klyuchko",
            "email": "avkluchko@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-openssl": "*",
        "symfony/config": "^3.4 || ^4.0 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.0 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "avkluchko/government-bundle": "^1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "AVKluchko\\X509Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AVKluchko\\X509Bundle\\Tests\\": "tests/"
        }
    }
}
