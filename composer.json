{
    "name": "avkluchko/x509-bundle",
    "description": "Bundle for work with x509 certificate",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Klyuchko",
            "email": "avkluchko@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-openssl": "*",
        "symfony/config": "^4.0 || ^5.1.5",
        "symfony/dependency-injection": "^4.1.12 || ^5.1.5",
        "symfony/http-kernel": "^4.0 || ^5.1.5",
        "avkluchko/government-bundle": "^1.1.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1.12 || ^5.1.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "AVKluchko\\X509Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AVKluchko\\X509Bundle\\Tests\\": "tests/"
        }
    }
}
